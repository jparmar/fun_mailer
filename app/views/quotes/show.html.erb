<p id="notice"><%= notice %></p>

<div class="row">
	<div class="col-md-9">
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3><b>Name</b></h3>
			</div>
			<div class="panel-body">
				<h4> <%= @quote.name %></h4>
			</div>
			<div class="panel-footer">
				<div class="btn btn-default"><%= link_to 'Edit', edit_quote_path(@quote), {:style=>'color:orange;'} %></div>
				<div class="btn btn-default"><%= link_to 'Back', quotes_path, {:style=>'color:black;'} %></div>
			</div>
		</div>	
	</div>	 
</div>


